{
    "manifest_version": 3,
    "name": "My Button Extension",
    "version": "1.0",
    "description": "Adds a button to all websites.",

    "icons": {
      "16": "icon-16.png",  
      "32": "icon-32.png",  
      "48": "icon-48.png",  
      "128": "icon-128.png"  
    },
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["./content.js"],
        "css": ["./a.css"]
      }
    ]
  }
  